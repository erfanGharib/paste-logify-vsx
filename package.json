{
  "name": "paste-logify",
  "displayName": "Paste Logify",
  "description": "Paste variables and auto-generate console logs.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onCommand:extension.pasteLogify"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.0.0",
    "vscode-test": "^1.6.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.pasteLogify",
        "title": "Custom Smart Paste"
      }
    ],
    "keybindings": [
      {
        "command": "extension.pasteLogify",
        "key": "ctrl+alt+v",
        "when": "editorTextFocus"
      }
    ]
  }
}